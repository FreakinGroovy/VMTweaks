"VertexlitGeneric"
{
	"$baseTexture" "models/weapons/c_items/c_robo_sandwich"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.5 .5 .5]"

	"$phong"	"1"
	"$phongexponent"	"1"
	"$phongboost"	"2.5"
	"$phongtint" "[1 0.8 1.2]"
	"$phongfresnelranges"	"[.25 .5 5]"
	"$lightwarptexture" "models\lightwarps\robot_lightwarp"
	"$halflambert" "1"

	"$basemapalphaphongmask" 1
	"$basealphaenvmapmask" "1" 

	"$rimlight"	"1"
	"$rimlightexponent"	"1"
	"$rimlightboost" ".25"

	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"
	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"
	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis" {}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}