"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_models/c_rocketlauncher/c_rocketlauncher_gold"
	"$phongexponenttexture" "models/weapons/c_models/c_rocketlauncher/c_rocketlauncher_exp"

	//"$envmap" "cubemaps/cubemap_gold001"	
	"$envmap" "env_cubemap"
	"$envmaptint" "[3 3 .25]"

	"$phong" "1"
	"$phongexponentfactor" "40"
	"$phongboost" "10"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".25"
	"$rimmask" "1"

	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"
	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"
	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis" {}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}