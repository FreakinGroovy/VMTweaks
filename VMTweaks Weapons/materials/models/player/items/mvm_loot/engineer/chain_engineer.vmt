"VertexLitGeneric"
{
	"$basetexture"	"models/player/items/mvm_loot/engineer/chain_engineer"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "10"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$halflambert" "1"

	"$basemapalphaphongmask" "1"

	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".25"
	
	"$cloakPassEnabled" "1"
	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis" {}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}