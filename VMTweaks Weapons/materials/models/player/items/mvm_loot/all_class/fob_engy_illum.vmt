"VertexLitGeneric"
{
	"$basetexture"	"models/player/items/mvm_loot/all_class/fob_engy"
	
	"$phong" "1"
	"$phongexponent"	"50"
	"$phongboost" ".1"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[1.5 .5 3]"
	"$halflambert" "1"

	"$basemapalphaphongmask" "1"

	"$selfillum" "1"

	"$cloakPassEnabled" "1"
	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis" {}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}