"VertexLitGeneric"
{
	"$baseTexture" "models/weapons/c_models/c_grenadelauncher/c_grenadelauncher_gold"

	"$envmap" "cubemaps/cubemap_gold001"	
	"$envmaptint" "[3 3 1]"

	"$phong" "1"
	"$phongexponent" "100"
	"$phongboost" "2"
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"

	"$basemapalphaphongmask" "1"

	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"
	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"
	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis" {}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}