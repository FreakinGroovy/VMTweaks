"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_models/c_ubersaw/c_ubersaw"

	"$phong" "1"
	"$phongexponent" "30"
	"$phongboost" "10"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges" "[0.25 0.5 1]"
	"$halflambert" "1"

	"$basemapalphaphongmask" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "3"

	"$selfillum" "1"
	"$selfillumtint" "[1 .11 .05]"
	"$selfillummask" "models/weapons/c_models/c_ubersaw/c_ubersaw_selfillummask"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"
	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis" {}
		"WeaponSkin" {}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}