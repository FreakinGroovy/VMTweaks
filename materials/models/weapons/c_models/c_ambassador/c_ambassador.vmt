"VertexLitGeneric"
{
	$baseTexture "models/weapons/c_models/c_ambassador/c_ambassador"
	$bumpmap "models/weapons/c_models/c_ambassador/c_ambassador_normal"

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "10"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongwarptexture"  "models/weapons/c_models/c_ambassador/c_ambassador_phongwarp"
	"$phongfresnelranges"	"[1.5 .5 3]"
	"$halflambert" "1"

	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".25"

	"$normalmapalphaenvmapmask" 1
	
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		//"WeaponSkin" {}
	}
}