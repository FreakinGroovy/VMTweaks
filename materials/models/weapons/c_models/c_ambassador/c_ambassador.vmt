"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_models/c_ambassador/c_ambassador"
	"$phongwarptexture"  "models/weapons/c_models/c_ambassador/c_ambassador_phongwarp"	

	"$phong" "1"	
	"$phongexponentfactor"  "80"
	"$lightwarptexture" "models/lightwarps/softened_weapon_lightwarp"	
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" "0.25"
	"$rimmask" "1"

	"$basemapalphaphongmask" "1"
	"$normalmapalphaenvmapmask" 1
	"$ambientocclusion" "1"
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		//"WeaponSkin" {}
	}
}
