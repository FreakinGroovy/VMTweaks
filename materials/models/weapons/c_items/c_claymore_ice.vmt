"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_claymore_ice"
           
	"$alpha" ".5"
	"$translucent" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.3 .3 .2]"
		
	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "10"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"

	//"$basemapalphaphongmask" "1"

	"$rimlight" "1"						
	"$rimlightexponent" "4"				
	"$rimlightboost" "2"

	"$cloakPassEnabled" "1"
	"$glowcolor" "0"
	"$yellow" "0"

	"Proxies"
	{
		"invis" {}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}