"VertexLitGeneric"
{
	"$baseTexture" "models\weapons\v_pda_engineer/v_pda_engineer_screen"

	"$basealphaenvmapmask" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.2 .2 .2]"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "5"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"

	"$basemapalphaphongmask" "1"

	"$cloakPassEnabled" "1"
	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis" {}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}